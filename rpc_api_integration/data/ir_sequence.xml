<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="api_customer_data_queue_sequence" model="ir.sequence">
        <field name="name">Sequence Customer Queue</field>
        <field name="code">partner.data.queue.ept</field>
        <field name="prefix">CDQ</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
    <record id="ir_cron_res_partner_queue" model="ir.cron">
        <field name="name">Customer Process Queue</field>
        <field name="active" eval="False"/>
        <field name="model_id" ref="model_res_partner_queue"/>
        <field name="state">code</field>
        <field name="code">model.run_customer_queue()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">1</field>
    </record>
