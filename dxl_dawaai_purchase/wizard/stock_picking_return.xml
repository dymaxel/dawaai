<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_return_picking_form_inherit_dawaai" model="ir.ui.view">
        <field name="name">stock.return.picking.inherit.dawaai</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
        <field name="model">stock.return.picking</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_return_moves']/tree/field[@name='quantity']" position="after">
                <field name="pack_qty"/>
                <field name="purchase_uom"/>
            </xpath>
        </field>
    </record>
</odoo>
